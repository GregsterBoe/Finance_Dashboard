{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "dev:debug": "concurrently \"npm run backend:debug\" \"npm run frontend\"",
    "backend": "cd ../backend && uvicorn app:app --reload --port 8000",
    "backend:debug": "cd ../backend && python -m debugpy --listen 5678 -m uvicorn app:app --reload --host 127.0.0.1 --port 8000",
    "frontend": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "proxy": "http://localhost:8000"
  },
  "dependencies": {
    "concurrently": "^9.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tanstack/react-query": "^5.0.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^9.8.8",
    "axios": "^1.6.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^7.0.39",
    "recharts": "^2.15.4",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
